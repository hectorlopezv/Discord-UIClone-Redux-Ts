nnels\n            getChannelList(setchannels);\n        });\n        \n\n    }, [getChannelList]);\n\n    return (\n        <div className=\"sidebar\">\n            <div className=\"sidebar_top\">\n                <h3>Hector Ui Clone</h3>\n                <ExpandMoreIcon />\n            </div>\n\n            <div className=\"sidebar_channels\">\n                <div className=\"sidebar_channelsHeader\">\n                    <div className=\"sidebar__header\">\n                        <ExpandMoreIcon />\n                        <h4>Text Channels</h4>\n                    </div>\n                    <AddIcon onClick={addChannelHandler} className=\"sidebar_addChannel\" />\n                </div>\n\n                <div className=\"sidebar_channelsList\">\n                    \n                    {channels.map(({ id, name }) => (\n                        <SidebarChannel key={id} id={id} channelName={name}\n                        />))}\n                </div>\n            </div>\n\n            <div className=\"sidebar_voice\">\n                <SignalCellularAltIcon\n                    fontSize=\"large\"\n                    className=\"sidebar_voiceIcon\" />\n                <div className=\"sidebar_voiceInfo\">\n                    <h3>Voice Connected</h3>\n                    <p>Stream</p>\n                </div>\n\n                <div className=\"sidebar_voiceIcons\">\n                    <CallIcon />\n                    <InfoOutlinedIcon />\n                </div>\n            </div>\n\n            <div className=\"sidebar_profile\">\n                <Avatar onClick={() => auth.signOut()} src={user ? user.photo : null} />\n                <div className=\"sidebar_profileInfo\">\n                    <h3>{user ? user.displayName : null}</h3>\n                    <p>#{user ? user.uid.substring(0, 5) : null}</p>\n                </div>\n\n                <div className=\"sidebar_profileIcons\">\n                    <MicIcon />\n                    <HeadsetIcon />\n                    <SettingsIcon />\n                </div>\n\n            </div>\n\n\n\n        </div>\n    );\n}\n\nexport default SideBar;","/home/hvmlopez/discord-ui-clone-ts/src/store/sagas/index.tsx",[],"/home/hvmlopez/discord-ui-clone-ts/src/store/sagas/discord.tsx",[],"/home/hvmlopez/discord-ui-clone-ts/src/axios.tsx",[],{"ruleId":"82","replacedBy":"83"},{"ruleId":"84","replacedBy":"85"},{"ruleId":"82","replacedBy":"83"},{"ruleId":"84","replacedBy":"85"},{"ruleId":"86","severity":1,"message":"87","line":14,"column":8,"nodeType":"88","messageId":"89","endLine":14,"endColumn":10},"no-native-reassign",["90"],"no-negated-in-lhs",["91"],"@typescript-eslint/no-unused-vars","'db' is defined but never used.","Identifier","unusedVar","no-global-assign","no-unsafe-negation"]